{
  "recursion_limit": 100,
  "tags": ["evaluation", "multidoc2dial", "indexing"],
  "configurable": {
    "corpus": {
      "docs_path": "examples/evaluation/data/md2d_docs/",
      "chunk_size": 512,
      "chunk_overlap": 64
    },
    "retrieval": {
      "embedding_method": "embedding:openai:text-embedding-3-small"
    },
    "vector_store": {
      "type": "pinecone",
      "pinecone": {
        "index_name": "orcheo-md2d",
        "namespace": "md2d_eval"
      }
    }
  }
}
