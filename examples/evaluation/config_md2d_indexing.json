{
  "recursion_limit": 100,
  "tags": ["evaluation", "multidoc2dial", "indexing"],
  "configurable": {
    "corpus": {
      "docs_path": "https://raw.githubusercontent.com/doc2dial/sharedtask-dialdoc2021/master/data/doc2dial/v1.0.1/doc2dial_doc.json",
      "max_documents": null,
      "chunk_size": 512,
      "chunk_overlap": 64
    },
    "retrieval": {
      "embed_model": "openai:text-embedding-3-small",
      "dimensions": 512
    },
    "indexing": {
      "batch_size": 32,
      "max_retries": 2,
      "backoff_seconds": 0.05,
      "skip_unchanged": false
    },
    "vector_store": {
      "type": "pinecone",
      "pinecone": {
        "index_name": "orcheo-md2d",
        "namespace": "md2d_eval"
      }
    }
  }
}
