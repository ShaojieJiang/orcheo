{
  "recursion_limit": 100,
  "tags": ["evaluation", "multidoc2dial"],
  "configurable": {
    "md2d": {
      "data_path": "examples/evaluation/data/md2d_validation.json",
      "max_conversations": 2
    },
    "retrieval": {
      "embed_model": "openai:text-embedding-3-small",
      "top_k": 4
    },
    "vector_store": {
      "type": "pinecone",
      "pinecone": {
        "index_name": "orcheo-md2d",
        "namespace": "md2d_eval"
      }
    },
    "generation": {
      "model": "openai:gpt-4o-mini"
    }
  }
}
